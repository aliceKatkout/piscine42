#include <stdlib.h>
#include <stdio.h>

int	ft_ultimate_range(int **range, int min, int max)
{
	int	size;
	
	if (min >= max)
		return (*(NULL));
	size = max - min;
	*range = (*int)malloc(sizeof(int) * size);
	if (*range == NULL)
		return (-1);
		
	
}
